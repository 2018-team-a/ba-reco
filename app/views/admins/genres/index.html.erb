<h1>Admins::Genres#index</h1>

<%= form_tag admins_genres_path, :method => 'get' do %>
    <p>
      <%= text_field_tag :search, params[:search] %>
      <%= submit_tag "検索", :name => nil %>
    </p>
<% end %>

<% @genres_search.each do |f| %>
<%= link_to f.name, admins_genre_path(f.id) %>
<%end%>

<h3><%= link_to "登録ページ", new_admins_genre_path %></h3>

<%= paginate @genres_search %>
